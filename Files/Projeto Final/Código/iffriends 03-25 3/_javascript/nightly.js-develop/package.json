{
  "name": "nightly.js",
  "version": "1.1.0",
  "description": "A zero dependency javascript library that enables the night mode in your website easily ",
  "main": "dist/nightly.min.js",
  "directories": {
    "doc": "doc",
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "uglifyjs src/nightly.js -c -m --source-map -o dist/nightly.min.js",
    "test": "karma start",
    "coverage": "istanbul cover _mocha test/nightly.test.js -x test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fcmam5/nightly.js.git"
  },
  "keywords": [
    "dark",
    "night",
    "ui",
    "frontend",
    "darkmode"
  ],
  "author": "Fortas Abdeldjalil",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Fcmam5/nightly.js/issues"
  },
  "homepage": "https://github.com/Fcmam5/nightly.js#readme",
  "devDependencies": {
    "chai": "4.1.2",
    "istanbul": "0.4.5",
    "karma": "2.0.3",
    "karma-chai-plugins": "0.9.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "mocha": "^5.2.0",
    "uglify-es": "^3.3.10"
  },
  "dependencies": {}
}
